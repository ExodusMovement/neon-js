{
  "packages": [
    "packages/*"
  ],
  "version": "5.0.2",
  "npmClient": "yarn",
  "useWorkspaces": true,
  "workspaces": [
    "packages/*"
  ],
  "command": {
    "version": {
      "allowBranch": [
        "master",
        "next",
        "archive/*",
        "exodus"
      ]
    },
    "publish": {
      "allowBranch": [
        "master",
        "next",
        "archive/*",
        "exodus"
      ]
    }
  }
}
